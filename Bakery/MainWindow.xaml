<Window x:Class="Bakery.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bakery"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="59" Stroke="Black" VerticalAlignment="Top" Width="800" Fill="#FFFFBDE8"/>
        <TextBlock HorizontalAlignment="Left" Margin="75,14,0,0" TextWrapping="Wrap" Text="ПекарНЯ" VerticalAlignment="Top" FontSize="24" Height="29" Width="282"/>
        <Button x:Name="BackButton" Content="Назад" HorizontalAlignment="Left" Margin="686,15,0,0" VerticalAlignment="Top" Background="#FFF536FF" FontSize="14" Height="31" Width="80" Click="BackButton_Click"/>
        <Frame x:Name="MainFrame" Content="" Margin="0,59,0,0" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
